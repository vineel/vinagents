{
  "name": "accordli-api",
  "version": "1.0.0",
  "description": "Node.js + Express + TypeScript REST API with PostgreSQL",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "manualdev": "PORT=4000;tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:worker": "tsx src/worker/index.ts",
    "dev:worker": "tsx watch src/worker/index.ts",
    "db:seed": "tsx src/db/seed.ts",
    "db:setup-worker": "tsx src/scripts/setup-worker-schema.ts",
    "test": "vitest run",
    "test-all": "npm test -- --run src/tests/agent.*.test.ts",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "nodejs",
    "express",
    "typescript",
    "postgresql",
    "prisma",
    "rest-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.1",
    "graphile-worker": "^0.16.6",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg": "^8.13.1",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.16.11",
    "@types/pg": "^8.11.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "supertest": "^7.2.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
